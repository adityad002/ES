{
  "name": "edu-scheduler",
  "version": "1.0.0",
  "description": "Educational Scheduling System",
  "main": "index.js",
  "scripts": {
    "start": "cd backend && node index.js",
    "server": "cd backend && nodemon index.js",
    "client": "cd client && npm start",
    "seed": "cd backend && node seedData.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd backend && npm install && cd ../client && npm install"
  },
  "keywords": [
    "timetable",
    "scheduling",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
} 